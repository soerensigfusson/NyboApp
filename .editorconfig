; http://editorconfig.org
root = true

[*]
indent_style = space
indent_size = 4
end_of_line = lf
charset = utf-8
trim_trailing_whitespace = true
insert_final_newline = true

[lib/ini.js]
indent_style = tab

[*.js]
indent_size = 4
indent_style = space

[*.md]
indent_size = 4

[node_modules/**.js]
codepaint = false

[{package.json,.travis.yml}]
indent_size = 2
indent_style = space
